<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>search</servlet-name>
        <servlet-class>search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listkategori</servlet-name>
        <servlet-class>listkategori</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listtask</servlet-name>
        <servlet-class>listtask</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editstatustugas</servlet-name>
        <servlet-class>editstatustugas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>search</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listkategori</servlet-name>
        <url-pattern>/listkategori</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listtask</servlet-name>
        <url-pattern>/listtask</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editstatustugas</servlet-name>
        <url-pattern>/editstatustugas</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
